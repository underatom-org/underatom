    "@typescript-eslint/parser": "^6.9.1",
    "@vitejs/plugin-react": "^2.1.0",
    "axe-playwright": "^2.0.1",
    "axios": "^1.7.7",
    "cmdk": "^0.2.1",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
